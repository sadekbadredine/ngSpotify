{
  "/api/": {
      "target": "https://accounts.spotify.com",
      "secure": false,
      "logLevel": "debug",
      "changeOrigin": true,
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS, PUT, PATCH, DELETE",
        "Access-Control-Allow-Headers": "X-Requested-With, content-type, token, language"
      },
      "pathRewrite": {
        "^/api": ""
      }
  }
}